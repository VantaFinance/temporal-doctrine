{
    "name": "vanta/temporal-doctrine",
    "description": "Integration temporal with doctrine",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "temporal",
        "library",
        "orm",
        "database",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Vlad Shashkov",
            "email": "v.shashkov@pos-credit.ru"
        },
        {
            "name": "Vanta Team",
            "homepage": "https://vanta.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vanta\\Integration\\Temporal\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanta\\Integration\\Temporal\\Doctrine\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "temporal/sdk": "^2.7",
        "doctrine/orm": "^2|^3"
    },
    "require-dev": {
        "sentry/sentry": "^3|^4",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.46",
        "phpstan/extension-installer": "^1.3",
        "phpunit/phpunit": "^10.5",
        "symfony/var-dumper": "^6.0",
        "doctrine/dbal": "^4.2",
        "monolog/monolog": "^3.9",
        "symfony/cache": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
